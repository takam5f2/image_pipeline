<launch>
  <node pkg="spinnaker_camera_nodes" exec="spinnaker_camera_node" name="spinnaker_camera_node" output="screen">
    <param name="width" value="1440" />
    <param name="height" value="1080" />
    <param name="fps" value="10" />
    <param name="dltl" value="100000000" />
    <param name="serial_number" value="0" />
    <param name="frame_id" value="camera_optical_link" />
    <param name="use_external_trigger" value="false" />
    <param name="external_trigger_gpio" value="2" />
    <param name="min_shutter_speed_ms" value="-1" />
  </node>
</launch>
